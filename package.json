{
  "name": "tswebpackplay",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "karma start",
    "clean-dist": "rimraf dist",
    "copy-files": "echo nothing to copy",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod",
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dev-server --env.dev --content-base dist",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "validate": "npm-run-all --parallel validate-webpack:* lint test"
  },
  "author": "Royston Shufflebotham",
  "devDependencies": {
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-remap-istanbul": "^0.1.0",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.11.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.0-dev.20160707",
    "typings": "^1.3.0",
    "webpack": "^2.1.0-beta.15",
    "webpack-dev-server": "^1.14.1",
    "webpack-validator": "^2.2.0"
  }
}
